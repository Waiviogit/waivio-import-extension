{
  "name": "react-chrome-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "antd": "^5.26.0",
    "archiver": "^7.0.1",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.9.0",
    "cheerio": "^1.1.2",
    "franc-min": "^6.2.0",
    "html2canvas": "^1.4.1",
    "joi": "^17.13.3",
    "qs": "^6.14.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-draggable": "^4.4.6",
    "styled-components": "^6.1.18",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/chrome": "0.0.326",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.0.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/styled-components": "^5.1.34",
    "@types/qs": "^6.14.0",
    "copy-webpack-plugin": "^13.0.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "react-scripts": "5.0.1",
    "ts-loader": "^9.5.2",
    "typescript": "^4.9.5",
    "typescript-eslint": "8.34.0",
    "webpack": "^5.99.9",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1"
  },
  "scripts": {
    "start": "webpack serve --config webpack.dev.js",
    "build": "webpack",
    "build:watch": "webpack --watch",
    "build:stats": "webpack --json > dist/stats.json",
    "analyze": "npm run build:stats && webpack-bundle-analyzer dist/stats.json",
    "lint": "eslint --fix --ext=.jsx,.js,.tsx,.ts  .",
    "dist-to-aws": "node ./deploy/index.js",
    "deploy": "npm run build && npm run dist-to-aws",
    "update-browser-list": "npx update-browserslist-db@latest",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
